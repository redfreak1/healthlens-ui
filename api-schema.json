                        "$ref": "#/components/schemas/PersonaType"
                    },
                    "confidence": {
                        "type": "number",
                        "title": "Confidence"
                    },
                    "reasoning": {
                        "type": "string",
                        "title": "Reasoning"
                    }
                },
