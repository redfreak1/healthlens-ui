                },
                "type": "object",
                "required": [
                    "name",
                    "value",
                    "unit",
                    "reference_range",
                    "category",
                    "status"
                ],
                "title": "LabResult"
            },
            "LabResultStatus": {
                "type": "string",
                "enum": [
                    "normal",
                    "high",
                    "low"
                ],
                "title": "LabResultStatus"
            },
            "PersonaResult": {
                "properties": {
